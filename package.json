{
  "name": "tidy-ai",
  "version": "1.0.0",
  "description": "Organize your Downloads folder with AI - a local-first CLI app",
  "main": "dist/server/server/index.js",
  "bin": {
    "tidyai": "./dist/cli/cli/index.js"
  },
  "files": [
    "dist/",
    ".next/",
    "public/",
    "scripts/postinstall.js",
    "README.md",
    "QUICKSTART.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "npm run build:next && npm run build:cli && npm run build:server",
    "build:next": "next build",
    "build:cli": "tsc --project tsconfig.cli.json && chmod +x dist/cli/cli/index.js",
    "build:server": "tsc --project tsconfig.server.json",
    "start": "next start",
    "lint": "next lint",
    "cli": "node dist/cli/index.js",
    "prepublishOnly": "npm run build",
    "postinstall": "node scripts/postinstall.js || true",
    "prepack": "npm run build",
    "prepublish-check": "node scripts/prepublish-check.js"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "file-organizer",
    "file-management",
    "downloads-organizer",
    "ai",
    "ollama",
    "cli",
    "macos",
    "automation",
    "file-sorter",
    "tidy"
  ],
  "author": "Tew12345678910",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tew12345678910/Ai-file-management.git"
  },
  "bugs": {
    "url": "https://github.com/Tew12345678910/Ai-file-management/issues"
  },
  "homepage": "https://github.com/Tew12345678910/Ai-file-management#readme",
  "dependencies": {
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "better-sqlite3": "^12.6.2",
    "chalk": "^4.1.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "commander": "^11.1.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "http-proxy-middleware": "^2.0.6",
    "lucide-react": "^0.344.0",
    "next": "^14.2.0",
    "openai": "^4.104.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwind-merge": "^2.2.1",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.2.0",
    "postcss": "^8.4.33",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3"
  }
}
